{
  "compilerOptions": {
    "target": "es2023",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["chrome"],
    "noEmit": true
  },
  "include": ["**/*.js", "**/*.mjs", "**/*.d.ts"],
  "exclude": ["node_modules"]
}
